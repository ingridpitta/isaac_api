    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "moment": "^2.29.1",
    "mongoose": "^5.12.3",
    "npm-run-all": "^4.1.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.7"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
